<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF6E9"
    tools:ignore="ExtraText"
    tools:context=".edit_insuranceActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!-- Left Arrow Image -->
            <ImageView
                android:id="@+id/leftArrow"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:contentDescription="@string/back"
                android:src="@drawable/left_arrow_black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

            <View
                android:id="@+id/view7"
                android:layout_width="276dp"
                android:layout_height="63dp"
                android:layout_margin="16dp"
                android:background="@drawable/violet_rectangle"
                android:rotation="6.07"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.091" />

            <View
                android:layout_width="276dp"
                android:layout_height="63dp"
                android:layout_margin="16dp"
                android:background="@drawable/rounded_rectangle"
                android:rotation="-4.48"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.091" />

            <TextView
                android:id="@+id/textView46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="91dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="center"
                android:text="@string/insurance_details"
                android:textColor="#FFFFFF"
                android:textSize="33sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editText8"
                android:layout_width="350dp"
                android:layout_height="60dp"
                android:layout_gravity="start"
                android:layout_marginTop="44dp"
                android:background="@drawable/et_blue"
                android:drawableStart="@drawable/insurance_provider"
                android:drawablePadding="10dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="start|center_vertical"
                android:hint="@string/provider_of_insurance"
                android:paddingStart="20dp"
                android:paddingTop="5dp"
                android:paddingEnd="31dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/black"
                android:textColorHint="#808080"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.491"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view7"
                tools:ignore="Autofill,MissingConstraints,TextFields" />

            <EditText
                android:id="@+id/editText9"
                android:layout_width="350dp"
                android:layout_height="60dp"
                android:layout_gravity="start"
                android:background="@drawable/et_blue"
                android:drawableStart="@drawable/chip_id"
                android:drawablePadding="10dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="start|center_vertical"
                android:hint="@string/policy_number"
                android:paddingStart="20dp"
                android:paddingTop="5dp"
                android:paddingEnd="31dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/black"
                android:textColorHint="#808080"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/editText8"
                app:layout_constraintHorizontal_bias="0.392"
                app:layout_constraintStart_toStartOf="@+id/editText8"
                tools:ignore="Autofill,MissingConstraints,TextFields"
                tools:layout_editor_absoluteY="270dp" />

            <EditText
                android:id="@+id/editText10"
                android:layout_width="350dp"
                android:layout_height="60dp"
                android:layout_gravity="start"
                android:layout_marginTop="36dp"
                android:background="@drawable/et_blue"
                android:drawableStart="@drawable/dob_calendar"
                android:drawablePadding="10dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="start|center_vertical"
                android:hint="@string/insured_date"
                android:importantForAutofill="no"
                android:inputType="date"
                android:paddingStart="20dp"
                android:paddingTop="5dp"
                android:paddingEnd="31dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/black"
                android:textColorHint="#808080"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/editText9"
                app:layout_constraintHorizontal_bias="0.215"
                app:layout_constraintStart_toStartOf="@+id/editText9"
                app:layout_constraintTop_toBottomOf="@+id/editText9"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/editText11"
                android:layout_width="350dp"
                android:layout_height="60dp"
                android:layout_gravity="start"
                android:layout_marginTop="36dp"
                android:background="@drawable/et_blue"
                android:drawableStart="@drawable/peso"
                android:drawablePadding="10dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="start|center_vertical"
                android:hint="@string/coverage_amount"
                android:inputType="date"
                android:paddingStart="20dp"
                android:paddingTop="5dp"
                android:paddingEnd="31dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/black"
                android:textColorHint="#808080"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/editText10"
                app:layout_constraintHorizontal_bias="0.529"
                app:layout_constraintStart_toStartOf="@+id/editText10"
                app:layout_constraintTop_toBottomOf="@+id/editText10"
                tools:ignore="Autofill,MissingConstraints" />

            <EditText
                android:id="@+id/editText12"
                android:layout_width="350dp"
                android:layout_height="60dp"
                android:layout_gravity="start"
                android:layout_marginTop="32dp"
                android:background="@drawable/et_blue"
                android:drawableStart="@drawable/dob_calendar"
                android:drawablePadding="10dp"
                android:fontFamily="@font/crimson_text_semibold"
                android:gravity="start|center_vertical"
                android:hint="@string/payment_frequency"
                android:inputType="date"
                android:paddingStart="20dp"
                android:paddingTop="5dp"
                android:paddingEnd="31dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/black"
                android:textColorHint="#808080"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/editText11"
                app:layout_constraintHorizontal_bias="0.196"
                app:layout_constraintStart_toStartOf="@+id/editText11"
                app:layout_constraintTop_toBottomOf="@+id/editText11"
                tools:ignore="Autofill,MissingConstraints" />

            <Button
                android:id="@+id/button23"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/btn_background2"
                android:fontFamily="@font/crimson_text_bold"
                android:text="@string/save"
                android:textColor="#ffff"
                android:textSize="30sp"
                app:layout_constraintEnd_toEndOf="@+id/editText12"
                app:layout_constraintHorizontal_bias="0.588"
                app:layout_constraintStart_toStartOf="@+id/editText12"
                app:layout_constraintTop_toBottomOf="@+id/editText12"
                tools:ignore="MissingConstraints" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>