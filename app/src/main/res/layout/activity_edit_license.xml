<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF6E9"
    tools:ignore="ExtraText"
    tools:context=".edit_licenseActivity">

    <!-- Left Arrow Image -->
    <ImageView
        android:id="@+id/leftArrow"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="@string/back"
        android:src="@drawable/left_arrow_black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:layout_width="276dp"
        android:layout_height="63dp"
        android:layout_margin="16dp"
        android:background="@drawable/violet_rectangle"
        android:rotation="6.07"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.091" />

    <View
        android:layout_width="276dp"
        android:layout_height="63dp"
        android:layout_margin="16dp"
        android:background="@drawable/rounded_rectangle"
        android:rotation="-4.48"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.091" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="91dp"
        android:fontFamily="@font/crimson_text_semibold"
        android:gravity="center"
        android:text="License Details"
        android:textColor="#FFFFFF"
        android:textSize="33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_gravity="start"
        android:background="@drawable/et_blue"
        android:drawableStart="@drawable/license_id"
        android:drawablePadding="10dp"
        android:fontFamily="@font/crimson_text_semibold"
        android:gravity="start|center_vertical"
        android:hint="License ID"
        android:hintTextColor="@color/gray"
        android:paddingStart="20dp"
        android:paddingTop="5dp"
        android:paddingEnd="31dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="231dp" />

    <EditText
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_gravity="start"
        android:background="@drawable/et_blue"
        android:drawableStart="@drawable/location"
        android:drawablePadding="10dp"
        android:fontFamily="@font/crimson_text_semibold"
        android:gravity="start|center_vertical"
        android:hint="Issuing Location"
        android:hintTextColor="@color/gray"
        android:paddingStart="20dp"
        android:paddingTop="5dp"
        android:paddingEnd="31dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="333dp" />

    <EditText
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_gravity="start"
        android:background="@drawable/et_blue"
        android:drawableStart="@drawable/dob_calendar"
        android:drawablePadding="10dp"
        android:fontFamily="@font/crimson_text_semibold"
        android:gravity="start|center_vertical"
        android:hint="Date of Issuance"
        android:hintTextColor="@color/gray"
        android:inputType="date"
        android:paddingStart="20dp"
        android:paddingTop="5dp"
        android:paddingEnd="31dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="549dp" />

    <EditText
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_gravity="start"
        android:background="@drawable/et_blue"
        android:drawableStart="@drawable/dob_calendar"
        android:drawablePadding="10dp"
        android:fontFamily="@font/crimson_text_semibold"
        android:gravity="start|center_vertical"
        android:hint="Renewal Date"
        android:hintTextColor="@color/gray"
        android:inputType="date"
        android:paddingStart="20dp"
        android:paddingTop="5dp"
        android:paddingEnd="31dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="439dp" />

    <Button
        android:id="@+id/button23"
        android:layout_width="333dp"
        android:layout_height="60dp"
        android:background="@drawable/btn_background2"
        android:fontFamily="@font/crimson_text_bold"
        android:text="Save"
        android:textColor="#ffff"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="676dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>